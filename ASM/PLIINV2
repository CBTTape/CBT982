   GO: PROC OPTIONS(MAIN);
   /* THIS BABY WILL OUTPUT A STRING TO JOL */
   DCL UJPLIOP ENTRY OPTIONS(ASSEMBLER);
   %DCL OUTPUT ENTRY;
   %OUTPUT : PROC(CHAR) RETURNS(CHAR);
     DCL CHAR CHAR;
     RETURN('WORK='||CHAR||'; CALL UJPLIOP(WORK);');
   % END OUTPUT;
    OUTPUT('MACRO;');
    OUTPUT('END;');
   DCL WORK CHAR(100) VAR;
   WORK='STOP ''PLI ROUTINE RETURNED THIS'';';
   CALL UJPLIOP(WORK);
  OUTPUT('STOP ''THIS IS A LONG STRING WE ARE OUTPUTTING TO THE JOL COMP
      COMPILER. IT IS GREATER THAN 71 CHARACTERS'';');
   END;
