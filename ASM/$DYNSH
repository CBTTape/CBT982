           ENTRY RETN,INSTRUCT,PRINT  ******FOR LINK MAP ******   75128
RETN       DCB DDNAME=&DDINST,DSORG=PO,MACRF=W,                 80290  *
               DEVD=DA,RECFM=FB,LRECL=80,                              *
               BLKSIZE=&RETNSIZ                                    DASD
