MIINSTR  DCB   DDNAME=JOLCATLG,DSORG=PS,MACRF=R,DEVD=DA,         88036 .
               EODAD=INSTEOF,BUFL=2000,BLKSIZE=800,LRECL=80,     88036 .
               RECFM=FB
