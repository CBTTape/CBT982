FREE     DS    0H                                                88036
         CLI   0(R2),X'00'                                       88036
         BE    FREED                                             88036
         XC    S99AREA(12),S99AREA                               88036
         MVC   S99RB(2),=X'1402'                                  J40B
         LA    R0,S99TU                                           J40B
         ST    R0,S99TULST                                        J40B
         MVC   TUDDN(6),=Y(1,1,8)                                 J40B
         MVC   TUDDN+6(8),0(R2)                                   J40B
         LA    R2,8(,R2)                                          J40B
         LA    R0,TUDDN                                           J40B
         ST    R0,S99TU                                           J40B
         LA    R0,=Y(7,0)                                         J40B
