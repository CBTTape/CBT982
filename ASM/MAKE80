         GENSAVE CSECT=GO
         OPEN (IN,(INPUT),OUT,(OUTPUT))
READ     GET   IN,CARD
         PUT   OUT,CARD+1
         B     READ
EOF      CLOSE (IN,,OUT)
    GENRETN
         PRINT NOGEN
IN DCB DSORG=PS,DDNAME=IN,MACRF=GM,EODAD=EOF
OUT DCB DSORG=PS,DDNAME=OUT,MACRF=PM,DEVD=DA
         PRINT GEN
CARD  DS  CL81
    LTORG
    END
