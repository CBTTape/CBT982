 PROC 0  SCHED(JOL.SCHEDULE)
 SET PREF=&SYSUID
 CONTROL          MSG NOFLUSH PROMPT
 ALLOC FI($$JINCL)  DA('&PREF..&SCHED')  SHR REUSE
 CONTROL NOMSG
 DELETE '&SYSUID..JOL.TEMPSCHD'
 CONTROL MSG
 ATTR XXXYX LRECL(80) RECFM(F)
 ALLOC FILE(SCREEN) DA(*) REUS USING(XXXYX)
 FREE F(XXXYX)
 JOL '&PREF..JOL.SCHEDULE(GOSCHED)' SYN
 SET RC=&LASTCC
 IF &RC ¬= 0 THEN -
   DO
      WRITE JOL COULDNT CREATE THE SCHEDULE OF JOBS.
      WRITE RETURN CODE = &RC
      FREE F(SCREEN)
      EXIT CODE(16)
   END
 WRITE
 WRITE
 WRITE  JOL HAS NOW CREATED THE TEMPORARY DATA SET CONTAINING
 WRITE      THE JOBS TO BE SUBMITTED TODAY.
 WRITE
 WRITE  ENTER 'SUB' TO HAVE JOL COMPILE THE JOBS.
 WRITE  ENTER 'LIST' TO LOOK AT THE JOBS TO BE SUBMITTED
 WRITENR +
    >> ENTER SUB LIST OR END (HIT ENTER TO TERMINATE)
    READ &ANS
       DO WHILE &ANS ¬= END AND &ANS ¬=
       IF &ANS = SUB THEN -
       DO
          FREE F($$INCL)
          JOL '&SYSUID..JOL.TEMPSCHD'
          EXIT CODE(0)
       END
       IF &ANS = LIST THEN REPRO IDS('&SYSUID..JOL.TEMPSCHD') OF(SCREEN
       WRITENR +
     >> ENTER OPTION SUB LIST OR END (HIT ENTER TO TERMINATE)
       READ &ANS
       END
    END
EXIT: -
FREE F(SCREEN)
EXIT CODE(&RETCODE)
