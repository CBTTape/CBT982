\par                                                                         00370000
\par     if %list(%dayno-1)='HOL'           /* Was YESTERDAY a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose} */    00380000
\par     &  %list(%dayno)='WORK'            /* and TODAY a WORKDAY ?   */    00390000
\par     then do;                                                            00400000
