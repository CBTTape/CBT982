\par          %work=%dayno+1;               /* Set WORK to TOMORROW  */      00500000
\par          if %work=%lastday then do;    /* TOMORROW is the LAST  */      00510000
\par                                           DAY of the month      */      00520000
\par              write 'We could PREPARE a job here';                       00530000
\par              PREPARE B ;                                                00540000
\par          end;                                                           00550000
\par          %work=%work+1;                /* Check the NEXT Day    */      00560000
\par          if %work=%lastday then do;    /* The NEXT day is the   */      00570000
\par                                           LAST DAY of the month */      00580000
\par              write 'We could PREPARE a job here';                       00590000
