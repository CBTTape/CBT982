\f2\fs16\cf1\insrsid9269099  /* This is the SPECIAL member of the JOL.SCHEDULE Data Set.            00010000
\par                                                                         00020000
\par     It is examined every day, and can test if TOMORROW is a             00030000
\par     {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose}, or yesterday and so on.  See the examples below. */        00040000
\par                                                                         00050000
\par     if %holiday='YES'                                                   00060000
\par     then do;                                                            00070000
\par          write 'We could PREPARE a job here';                           00080000
\par     end;                                                                00090000
\par                                                                         00100000
\par /* See if the next few days are HOLIDAYS */                             00110000
\par                                                                         00120000
\par     if %holiday='NO'                                                    00130000
\par     then do;                                                            00140000
\par          if %list(%dayno+1)='HOL'                                       00150000
\par          then do;                      /* TOMORROW is a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose} */      00160000
\par              write 'We could PREPARE a job here';                       00170000
\par              PREPARE tomorrow;                                          00180000
\par              PREPARE B ;                                                00190000
\par          end;                                                           00200000
\par          if %list(%dayno+2)='HOL'                                       00210000
\par          then do;                      /* The NEXT day is a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose} */  00220000
\par              write 'We could PREPARE a job here';                       00230000
\par          end;                                                           00240000
\par          if %list(%dayno+3)='HOL'                                       00250000
\par          then do;                      /* and +3 day is a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose} */    00260000
\par              write 'We could PREPARE a job here';                       00270000
\par          end;                                                           00280000
\par          if %list(%dayno-1)='HOL'                                       00290000
\par          then do;                      /* YESTERDAY was a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose} */    00300000
\par              write 'We could PREPARE a job here';                       00310000
\par          end;                                                           00320000
\par     end;                                                                00330000
\par                                                                         00340000
\par }{\rtlch\fcs1 \af1\afs16 \ltrch\fcs0 \fs16\cf1\insrsid9269099 /* Sample ONLY:- See if Yesterday was a {\*\xmlopen\xmlns2{\factoidname PersonName}}Holiday{\*\xmlclose}, and Today is a         00350000
